{
	"name": "@cd/app-checkout-widget",
	"private": true,
	"version": "0.0.1",
	"description": "An embeddable javascript widget, built by Gras.",
	"main": "index.ts",
	"babel": {
		"presets": [
			"@babel/preset-react"
		]
	},
	"scripts": {
		"build": "NODE_ENV=production webpack-cli --mode production && dotenv -e ../../.env.proudction -- yarn build-storybook",
		"build-css": "tailwindcss -o ./src/styles/tailwind.css",
		"build-storybook": "build-storybook -c .storybook -o dist/storybook",
		"dev": "npm-run-all --parallel --print-label watch-tailwind dev-server",
		"dev-server": "NODE_ENV=development dotenv -e ../../.env.development -- webpack-dev-server",
		"storybook": "start-storybook -p 6006",
		"test:unit": "jest",
		"test:unit-ci": "jest --ci --runInBand --reporters=default --reporters=jest-junit",
		"test:coverage": "jest --coverage",
		"test:watch": "jest --watchAll",
		"watch-tailwind": "tailwindcss -o ./src/styles/tailwind.css --watch"
	},
	"dependencies": {
		"@cd/core-lib": "workspace:*",
		"@cd/data-access": "workspace:*",
		"@cd/eslint-config": "workspace:*",
		"@cd/ui-lib": "workspace:*",
		"cheerio": "1.0.0-rc.12",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-router-dom": "6.10.0",
		"tailwind-merge": "1.8.0"
	},
	"devDependencies": {
		"@babel/core": "7.21.4",
		"@babel/preset-react": "7.22.5",
		"@cfaester/enzyme-adapter-react-18": "0.7.0",
		"@storybook/addon-actions": "7.3.2",
		"@storybook/addon-links": "7.3.2",
		"@storybook/addons": "7.3.2",
		"@storybook/react": "7.3.2",
		"autoprefixer": "10.4.13",
		"babel-jest": "29.5.0",
		"babel-loader": "9.1.2",
		"copy-webpack-plugin": "11.0.0",
		"css-loader": "6.7.3",
		"daisyui": "2.51.6",
		"dotenv": "16.0.3",
		"dotenv-cli": "7.0.0",
		"dotenv-expand": "10.0.0",
		"enzyme": "3.11.0",
		"enzyme-adapter-react-16": "1.15.7",
		"enzyme-to-json": "3.6.2",
		"eslint": "8.29.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-import": "2.26.0",
		"eslint-plugin-jsx-a11y": "6.6.1",
		"eslint-plugin-prettier": "4.2.1",
		"eslint-plugin-react": "7.31.11",
		"eslint-plugin-simple-import-sort": "8.0.0",
		"javascript-obfuscator": "4.0.2",
		"jest": "29.3.1",
		"jest-environment-jsdom": "29.3.1",
		"jest-junit": "16.0.0",
		"npm-run-all": "4.1.5",
		"postcss": "8.4.21",
		"postcss-cli": "10.1.0",
		"postcss-import": "15.1.0",
		"postcss-loader": "7.2.4",
		"postcss-namespace-selector": "0.0.3",
		"postcss-preset-env": "8.3.1",
		"postcss-selector-namespace": "3.0.1",
		"prettier": "2.8.0",
		"style-loader": "3.3.2",
		"tailwindcss": "3.3.2",
		"ts-jest": "29.1.1",
		"ts-loader": "9.4.2",
		"tsconfig-paths-webpack-plugin": "4.1.0",
		"typescript": "4.9.3",
		"webpack": "5.78.0",
		"webpack-cli": "5.0.1",
		"webpack-dev-server": "4.13.2",
		"webpack-obfuscator": "3.5.1"
	}
}
