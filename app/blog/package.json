{
	"name": "@cd/app-blog",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"build": "yarn build:css && rm -rf ./.next && NODE_ENV=production NEXT_PUBLIC_IS_LOCAL_BUILD=1 SKIP_BUILD_STATIC_GENERATION=1 next build",
		"build:ci": "yarn build:css && rm -rf ./.next && NODE_ENV=production next build",
		"build:css": "tailwindcss -i ./src/styles/blog.css -o ./src/styles/tailwind.css",
		"clean": "rimraf ./src/styles/tailwind.css ./.next ./out ./dist ./build ./node_modules ./coverage ./tsconfig.tsbuildinfo ./node_modules/.cache",
		"debug": "NODE_OPTIONS='--inspect' next dev -p 3007",
		"dev": "npm-run-all --parallel --print-label watch-tailwind next-dev",
		"lint": "next lint",
		"lint:fix": "eslint . --ext .ts,.tsx --fix",
		"next-dev": "(npx local-ssl-proxy --config proxy.config.json) & next dev -p 3007",
		"test:unit": "jest --config ./jest.config.ts --passWithNoTests",
		"test:unit-ci": "jest --config ./jest.config.ts --ci --runInBand --reporters=default --reporters=jest-junit --passWithNoTests",
		"test:coverage": "jest --config ./jest.config.ts --coverage --passWithNoTests",
		"test:watch": "jest --config ./jest.config.ts --watchAll --passWithNoTests",
		"start": "node server.js",
		"watch-tailwind": "tailwindcss -i ./src/styles/blog.css -o ./src/styles/tailwind.css --watch"
	},
	"packageManager": "yarn@3.3.1",
	"browserslist": {
		"production": [
			">0.3%",
			"not ie 11",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"dependencies": {
		"@cd/ai": "workspace:*",
