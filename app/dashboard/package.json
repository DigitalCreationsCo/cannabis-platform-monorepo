	"browserslist": {
		"production": [
			">0.3%",
			"not ie 11",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"name": "@cd/app-dashboard",
	"private": true,
	"scripts": {
		"build": "yarn build:css && rm -rf ./.next && NODE_ENV=production next build",
		"build:ci": "yarn build:css && rm -rf ./.next && NODE_ENV=production next build",
		"build:css": "tailwindcss -i ./src/styles/dashboard.css -o ./src/styles/tailwind.css",
		"clean": "rimraf ./src/styles/tailwind.css ./.next ./out ./dist ./build ./node_modules ./coverage ./tsconfig.tsbuildinfo ./node_modules/.cache",
		"debug": "NODE_OPTIONS='--inspect' next dev -p 3001",
		"dev": "npm-run-all --parallel --print-label watch-tailwind next-dev",
		"lint": "next lint",
		"lint:fix": "eslint . --ext .ts,.tsx --fix",
		"next-dev": "next dev -p 3001",
		"test:unit": "jest",
		"test:unit-ci": "jest --ci --runInBand --reporters=default --reporters=jest-junit",
		"test:coverage": "jest --coverage",
		"test:watch": "jest --watchAll",
		"start": "PORT=3001 node server.js",
		"watch-tailwind": "tailwindcss -i ./src/styles/dashboard.css -o ./src/styles/tailwind.css --watch"
	},
	"version": "0.0.1"
}
