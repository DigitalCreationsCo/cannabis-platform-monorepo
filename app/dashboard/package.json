{
  "name": "@gras/dashboard",
  "version": "0.1.2",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "debug": "NODE_OPTIONS='--inspect' next dev -p 3001",
    "build": "rm -rf ./.next && yarn build:css && NODE_ENV=production next build",
    "build:ci": "rm -rf ./.next && yarn build:css && NODE_ENV=production next build",
    "build:css": "npm run copy-shared-assets && npx tailwindcss -i ./styles/globals.css -o ./styles/tailwind.css --minify",
    "watch:tailwind": "npx tailwindcss -i ./styles/globals.css -o ./styles/tailwind.css --minify --watch",
    "start": "PORT=3001 NEW_RELIC_APP_NAME=dashboard NEW_RELIC_LICENSE_KEY=6cf1545341d719f71ea4a2af51be9661FFFFNRAL node -r @newrelic/next server.js",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "check-lint": "eslint . --ext ts --ext tsx --ext js",
    "check-unused": "npx knip",
    "copy-shared-assets": "sh ../../scripts/copysharedCSSandAssets.sh",
    "clean": "rm -rf ./.next ./out ./dist ./build ./node_modules ./coverage ./tsconfig.tsbuildinfo",
    "format": "prettier --write .",
    "lint": "next lint",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "test-all": "npm run check-format && npm run check-lint && npm run check-types && npm run build",
    "playwright:update": "playwright install && playwright install-deps",
    "test:e2e": "playwright test",
    "test:unit": "jest",
    "test:unit-ci": "jest --config ./jest.config.js --ci --runInBand --reporters=default --reporters=jest-junit",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --env=jsdom",
    "sync-stripe": "node --env-file ../../.env.production sync-stripe.js",
    "sync-stripe-dev": "node --env-file ../../.env.development sync-stripe.js",
    "stripe:webhook": "stripe listen --forward-to localhost:3001/api/webhooks/stripe",
    "stripe:webhook-trigger": "stripe trigger customer.subscription.created",
    "check-locale": "node check-locale.js",
    "delete-team": "node --env-file ../../.env.production delete-team.js"
  },
  "packageManager": "yarn@3.3.1",
  "dependencies": {
    "@auth/mongodb-adapter": "3.0.0",
    "@boxyhq/metrics": "0.2.6",
    "@boxyhq/react-ui": "3.3.43",
    "@boxyhq/saml-jackson": "1.23.3",
    "@cd/core-lib": "workspace:*",
    "@cd/data-access": "workspace:*",
    "@cd/eslint-config": "workspace:*",
    "@cd/ui-lib": "workspace:*",
    "@fullcalendar/core": "^6.1.14",
    "@fullcalendar/daygrid": "^6.1.14",
    "@fullcalendar/react": "^6.1.14",
    "@heroicons/react": "2.1.3",
    "@newrelic/next": "^0.9.0",
    "@react-email/components": "0.0.16",
    "@react-email/render": "0.0.12",
    "@reduxjs/toolkit": "2.2.1",
    "@retracedhq/logs-viewer": "2.7.2",
    "@retracedhq/retraced": "0.7.9",
    "@sentry/nextjs": "7.112.0",
    "@stripe/react-stripe-js": "2.4.0",
    "@stripe/stripe-js": "2.3.0",
    "@tailwindcss/typography": "0.5.12",
    "bcryptjs": "2.4.3",
    "cookies-next": "4.1.1",
    "cron-to-human": "^1.0.0",
    "currency-symbol-map": "5.1.0",
    "daisyui": "4.10.2",
    "date-fns-tz": "3.1.3",
    "find-up": "5.0.0",
    "formik": "2.4.5",
    "framer-motion": "10.15.0",
    "i18next": "22.5.1",
    "micromatch": "4.0.5",
    "mixpanel-browser": "2.49.0",
    "mongodb": "5.8.1",
    "newrelic": "11.19.0",
    "next": "14.1.0",
    "next-auth": "4.24.7",
    "next-i18next": "13.3.0",
    "next-redux-wrapper": "8.1.0",
    "next-seo": "6.5.0",
    "nodemailer": "6.9.13",
    "react": "^18.3.1",
    "react-alice-carousel": "2.9.1",
    "react-daisyui": "5.0.0",
    "react-dom": "^18.3.1",
    "react-email": "2.1.1",
    "react-google-recaptcha": "3.1.0",
    "react-hot-toast": "2.4.1",
    "react-i18next": "12.3.1",
    "react-redux": "8.0.2",
    "redux-persist": "6.0.0",
    "slack-notify": "2.0.7",
    "stripe": "15.3.0",
    "svix": "1.21.0",
    "swr": "2.2.5",
    "tailwind-merge": "1.8.0",
    "tailwindcss": "3.4.3",
    "yup": "0.32.11",
    "zod": "3.23.3"
  },
  "devDependencies": {
    "@playwright/test": "1.43.1",
    "@types/bcryptjs": "2.4.6",
    "@types/jest": "^29.5.12",
    "@types/micromatch": "4.0.7",
    "@types/mixpanel-browser": "2.49.0",
    "@types/newrelic": "9.14.4",
    "@types/node": "20.12.7",
    "@types/nodemailer": "6.4.14",
    "@types/react": "18.2.79",
    "@types/react-google-recaptcha": "2.1.5",
    "autoprefixer": "10.4.19",
    "dotenv": "16.0.3",
    "dotenv-expand": "10.0.0",
    "eslint": "8.57.0",
    "file-loader": "6.2.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "node-loader": "2.0.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.38",
    "prettier": "3.2.5",
    "redux-devtools-extension": "2.13.9",
    "typescript": "5.4.5"
  }
}
