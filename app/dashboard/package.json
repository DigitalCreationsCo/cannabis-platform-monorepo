{
  "name": "@gras/dashboard",
  "version": "0.1.2",
  "private": true,
  "scripts": {
    "release": "git checkout release && git merge origin/main && release-it && git checkout main && git merge origin/release && git push origin main",
    "dev": "npm-run-all --parallel --print-label watch-tailwind next-dev",
    "next-dev": "next dev -p 3001",
    "debug": "NODE_OPTIONS='--inspect' next dev -p 3001",
    "build": "yarn build:css && rm -rf ./.next && NODE_ENV=production next build",
    "build:ci": "yarn build:css && rm -rf ./.next && NODE_ENV=production next build",
    "build:css": "tailwindcss -i ./styles/dashboard.css -o ./styles/tailwind.css",
    "start": "PORT=3001 node server.js",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "check-lint": "eslint . --ext ts --ext tsx --ext js",
    "check-unused": "npx knip",
    "clean": "rm -rf ./styles/tailwind.css ./.next ./out ./dist ./build ./node_modules ./coverage ./tsconfig.tsbuildinfo",
    "format": "prettier --write .",
    "lint": "next lint",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "test-all": "npm run check-format && npm run check-lint && npm run check-types && npm run build",
    "playwright:update": "playwright install && playwright install-deps",
    "test:e2e": "playwright test",
    "test": "jest",
    "test:unit-ci": "jest --config ./jest.config.js --ci --runInBand --reporters=default --reporters=jest-junit",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --env=jsdom",
    "sync-stripe": "node --env-file .env sync-stripe.js",
    "check-locale": "node check-locale.js",
    "delete-team": "node --env-file .env delete-team.js",
    "watch-tailwind": "tailwindcss -i ./styles/dashboard.css -o ./styles/tailwind.css --watch"
  },
  "packageManager": "yarn@3.3.1",
  "dependencies": {
    "@auth/mongodb-adapter": "3.0.0",
    "@boxyhq/metrics": "0.2.6",
    "@boxyhq/react-ui": "3.3.43",
    "@boxyhq/saml-jackson": "1.23.3",
    "@cd/core-lib": "workspace:*",
    "@cd/data-access": "workspace:*",
    "@cd/eslint-config": "workspace:*",
    "@cd/ui-lib": "workspace:*",
    "@heroicons/react": "2.1.3",
    "@react-email/components": "0.0.16",
    "@react-email/render": "0.0.12",
    "@reduxjs/toolkit": "1.9.2",
    "@retracedhq/logs-viewer": "2.7.2",
    "@retracedhq/retraced": "0.7.9",
    "@sentry/nextjs": "7.112.0",
    "@stripe/react-stripe-js": "2.4.0",
    "@stripe/stripe-js": "2.3.0",
    "@tailwindcss/typography": "0.5.12",
    "@typeform/embed-react": "2.28.0",
    "@vercel/og": "0.6.1",
    "bcryptjs": "2.4.3",
    "classnames": "2.5.1",
    "cookies-next": "4.1.1",
    "currency-symbol-map": "5.1.0",
    "date-fns-tz": "3.1.3",
    "find-up": "5.0.0",
    "formik": "2.2.9",
    "framer-motion": "10.15.0",
    "i18next": "22.5.1",
    "micromatch": "4.0.5",
    "mixpanel-browser": "2.49.0",
    "mongodb": "5.8.1",
    "next": "14.1.0",
    "next-auth": "4.24.7",
    "next-i18next": "13.3.0",
    "next-redux-wrapper": "8.1.0",
    "nodemailer": "6.9.13",
    "react": "18.2.0",
    "react-alice-carousel": "2.9.1",
    "react-confetti": "6.1.0",
    "react-daisyui": "5.0.0",
    "react-dom": "18.2.0",
    "react-email": "2.1.1",
    "react-google-recaptcha": "3.1.0",
    "react-hot-toast": "2.4.1",
    "react-i18next": "12.3.1",
    "react-js-cron": "5.0.1",
    "react-multi-carousel": "2.8.5",
    "react-redux": "8.0.2",
    "redux-persist": "6.0.0",
    "sharp": "0.33.3",
    "slack-notify": "2.0.7",
    "stripe": "15.3.0",
    "svix": "1.21.0",
    "swr": "2.2.5",
    "tailwind-merge": "1.8.0",
    "yup": "0.32.11",
    "zod": "3.23.3"
  },
  "devDependencies": {
    "@faker-js/faker": "8.4.1",
    "@playwright/test": "1.43.1",
    "@testing-library/jest-dom": "6.4.2",
    "@types/bcryptjs": "2.4.6",
    "@types/jest": "29.5.12",
    "@types/micromatch": "4.0.7",
    "@types/mixpanel-browser": "2.49.0",
    "@types/node": "20.12.7",
    "@types/nodemailer": "6.4.14",
    "@types/react": "18.2.79",
    "@types/react-google-recaptcha": "2.1.5",
    "@typescript-eslint/eslint-plugin": "5.45.0",
    "@typescript-eslint/parser": "5.45.0",
    "autoprefixer": "10.4.19",
    "daisyui": "4.10.2",
    "dotenv": "16.0.3",
    "dotenv-expand": "10.0.0",
    "eslint": "8.37.0",
    "eslint-config-next": "14.2.2",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-i18next": "6.0.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "file-loader": "6.2.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.38",
    "prettier": "3.2.5",
    "prettier-plugin-tailwindcss": "0.5.14",
    "redux-devtools-extension": "2.13.9",
    "redux-persist": "6.0.0",
    "release-it": "17.2.0",
    "tailwindcss": "3.4.3",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.2",
    "typescript": "4.9.3"
  }
}
