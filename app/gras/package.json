{
  "name": "@gras/shop",
  "version": "0.1.2",
  "private": true,
  "scripts": {
    "build": "rm -rf ./.next && cross-env NODE_ENV=production yarn build:css && cross-env NODE_ENV=production NEXT_PUBLIC_IS_LOCAL_BUILD=1 SKIP_BUILD_STATIC_GENERATION=1 ANALYZE=true next build",
    "build:ci": "rm -rf ./.next && cross-env NODE_ENV=production yarn build:css && cross-env NODE_ENV=production NEXT_PUBLIC_IS_LOCAL_BUILD=0 next build",
    "build:css": "npm run copy-shared-assets && npx tailwindcss -i ./styles/globals.css -o ./styles/tailwind.css --minify",
    "watch:css": "npx tailwindcss -i ./styles/globals.css -o ./styles/tailwind.css --minify --watch",
    "clean": "rm -rf ./.next ./.swc ./out ./dist ./build ./node_modules ./coverage ./tsconfig.tsbuildinfo",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "check-lint": "eslint . --ext ts --ext tsx --ext js",
    "check-unused": "npx knip",
    "copy-shared-assets": "sh ../../scripts/copysharedCSSandAssets.sh",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "dev": "(npx local-ssl-proxy --config proxy.config.json) & next dev",
    "format": "prettier --write .",
    "lint": "next lint",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "test-all": "npm run check-format && npm run check-lint && npm run check-types && npm run build",
    "playwright:update": "playwright install && playwright install-deps",
    "test:e2e": "playwright test",
    "test:unit": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --env=jsdom",
    "start": "NEW_RELIC_APP_NAME=gras NEW_RELIC_LICENSE_KEY=6cf1545341d719f71ea4a2af51be9661FFFFNRAL node -r @newrelic/next server.js",
    "preview": "cp -R .next/static .next/standalone/app/gras/.next && cd .next/standalone/app/gras && NEW_RELIC_APP_NAME=gras NEW_RELIC_LICENSE_KEY=6cf1545341d719f71ea4a2af51be9661FFFFNRAL node -r @newrelic/next server.js",
    "sync-stripe": "node --env-file .env sync-stripe.js",
    "check-locale": "node check-locale.js",
    "delete-team": "node --env-file .env delete-team.js",
    "sanity:deploy": "sanity deploy"
  },
  "packageManager": "yarn@3.3.1",
  "dependencies": {
    "@auth/mongodb-adapter": "3.0.0",
