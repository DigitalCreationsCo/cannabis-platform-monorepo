{
  "name": "@cd/app-drive",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "yarn build:css && rm -rf ./.next && NODE_ENV=production next build",
    "build:ci": "yarn build:css && rm -rf ./.next && NODE_ENV=production next build",
    "build:css": "tailwindcss -i ./src/styles/index.css -o ./src/styles/tailwind.css",
    "clean": "rm -rf ./src/styles/tailwind.css ./.next ./out ./dist ./build ./node_modules ./coverage ./tsconfig.tsbuildinfo",
    "debug": "NODE_OPTIONS='--inspect' next dev -p 3002",
    "dev": "npm-run-all --parallel --print-label watch-tailwind next-dev",
    "lint": "next lint",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "next-dev": "(npx local-ssl-proxy --config proxy.config.json) & next dev -p 3002",
    "test:unit": "jest --config ./jest.config.js",
    "test:unit-ci": "jest --config ./jest.config.js --ci --runInBand --reporters=default --reporters=jest-junit",
    "test:coverage": "jest --config ./jest.config.js --coverage",
    "test:watch": "jest --config ./jest.config.js --watchAll",
    "start": "PORT=3002 node server.js",
    "watch-tailwind": "tailwindcss -i ./src/styles/index.css -o ./src/styles/tailwind.css --watch"
  },
  "packageManager": "yarn@3.3.1",
  "browserslist": {
    "production": [
      ">0.3%",
      "not ie 11",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@cd/core-lib": "workspace:*",
    "@cd/data-access": "workspace:*",
    "@cd/eslint-config": "workspace:*",
    "@cd/ui-lib": "workspace:*",
    "@googlemaps/react-wrapper": "^1.1.35",
    "@paralleldrive/cuid2": "2.2.1",
    "@reduxjs/toolkit": "1.9.2",
    "@stripe/react-stripe-js": "2.4.0",
    "@stripe/stripe-js": "2.3.0",
    "@t3-oss/env-nextjs": "0.8.0",
    "@typeform/embed-react": "2.28.0",
    "axios": "1.2.1",
    "boarding.js": "3.5.2",
    "cors": "2.8.5",
    "date-fns-tz": "2.0.0",
    "find-up": "5.0.0",
    "form-data": "4.0.0",
    "framer-motion": "10.15.0",
    "json-immutable": "0.5.0",
    "next": "14.1.0",
    "next-connect": "0.13.0",
    "next-redux-wrapper": "8.1.0",
    "nextjs-cors": "2.1.2",
    "react": "18.2.0",
    "react-confetti": "6.1.0",
    "react-cookie": "4.1.1",
    "react-dom": "18.2.0",
    "react-google-recaptcha": "3.1.0",
    "react-hot-toast": "2.4.0",
    "react-redux": "8.0.2",
    "redis": "4.6.6",
    "sharp": "0.33.1",
    "socket.io-client": "4.7.4",
    "swr": "2.2.4",
    "tailwind-merge": "1.8.0",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "@next/env": "13.2.4",
    "@tailwindcss/typography": "0.5.8",
    "@testing-library/dom": "9.3.1",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/enzyme": "3.10.13",
    "@types/formidable": "2.0.5",
    "@types/google.maps": "^3.55.2",
    "@types/jest": "29.2.4",
    "@types/node": "18.11.10",
    "@types/react-dom": "18.0.11",
    "@types/react-google-recaptcha": "2.1.5",
    "@types/redux-mock-store": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "5.45.0",
    "@typescript-eslint/parser": "5.45.0",
    "autoprefixer": "10.4.13",
    "babel-jest": "29.5.0",
    "daisyui": "2.51.6",
    "dotenv": "16.0.3",
    "dotenv-cli": "7.0.0",
    "dotenv-expand": "10.0.0",
