	"private": true,
	"scripts": {
		"build": "rm -rf ./dist && dotenv -e ../../.env.production -- babel ./src --out-dir dist --extensions \".ts,.tsx\"",
		"build:ci": "rm -rf ./dist && dotenv -e ../../.env.production -- babel ./src --out-dir dist --extensions \".ts,.tsx\"",
		"clean": "rm -rf ./.next ./out ./dist ./build ./node_modules ./coverage ./tsconfig.tsbuildinfo ./node_modules/.cache",
		"start": "dotenv -e ../../.env.production -- pm2-runtime ecosystem.config.js",
		"test:unit": "echo \"Tests are not ready for use.\" && exit 0",
		"test:unit-ci": "echo \"Tests are not ready for use.\" && exit 0",
		"test:fix": "../../node_modules/jest/bin/jest.js --detectOpenHandles",
		"test:watch": "../../node_modules/jest/bin/jest.js --watchAll",
		"watch": "rm -rf ./node_modules/.cache && dotenv -e ../../.env.development -- babel-watch -e \".ts\" -c ./babel.config.js --watch ./src/worker ./src/index.ts"
	},
	"version": "0.0.1"
