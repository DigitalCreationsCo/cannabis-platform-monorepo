{
  "main": "./src/index.ts",
  "dependencies": {
    "@cd/core-lib": "workspace:*",
    "@cd/data-access": "workspace:*",
    "axios": "1.2.1",
    "bcryptjs": "2.4.3",
    "body-parser": "1.20.2",
    "busboy": "1.6.0",
    "cors": "2.8.5",
    "cross-env": "7.0.3",
    "express": "4.18.2",
    "find-up": "5.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.20.7",
    "@babel/core": "7.21.4",
    "@babel/node": "7.20.7",
    "@babel/preset-env": "7.21.4",
    "@babel/preset-typescript": "7.18.6",
    "@types/bcryptjs": "2.4.2",
    "@types/jest": "29.2.4",
    "@types/node": "18.11.10",
    "@typescript-eslint/eslint-plugin": "5.45.0",
    "@typescript-eslint/parser": "5.45.0",
    "babel-plugin-css-modules-transform": "1.6.2",
    "babel-plugin-inline-dotenv": "1.7.0",
    "babel-watch": "7.7.2",
    "concurrently": "8.0.1",
    "dotenv": "16.0.3",
    "dotenv-cli": "7.0.0",
    "dotenv-expand": "10.0.0",
    "eslint": "8.29.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "jest": "29.3.1",
    "nanoid": "3.3.4",
    "nodemon": "2.0.20",
    "pm2": "5.2.2",
    "prettier": "2.8.0",
    "rimraf": "4.4.1",
    "supertest": "6.3.3",
    "supertokens-node": "12.1.3",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "typescript": "4.9.3"
