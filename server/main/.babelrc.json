{
    "presets": [
      ["@babel/preset-env", { "modules": false, "targets": { "node": true } }],
      ["@babel/preset-typescript"]
    ],
    "ignore": ["node_modules"],
    "comments": false,
    "minified": true,
    "env": {
        "development": {
            "plugins": [
                [
                    "inline-dotenv",
                    {
                        "path": "../../.env.development"
                    }
                ]
            ]
        },
        "production": {
            "plugins": [
                [
                    "inline-dotenv",
                    {
                        "path": "../../.env.production"
                    }
                ]
            ]
        }
    }
  }