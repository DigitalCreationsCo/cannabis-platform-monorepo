{
    "presets": [
      ["@babel/preset-env"],
      ["@babel/preset-typescript"]
    ],
    "ignore": ["node_modules", "**/*.css"],
    "minified": true,
    "env": {
        "development": {
            "plugins": [
                [
                    "inline-dotenv",
                    {
                        "path": "../../.env.development"
                    }
                ]
            ]
        },
        "production": {
            "plugins": [
                [
                    "inline-dotenv",
                    {
                        "path": "../../.env.production"
                    }
                ]
            ]
        }
    }
  }