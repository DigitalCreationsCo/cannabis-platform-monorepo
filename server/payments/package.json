{
  "dependencies": {
    "@cd/core-lib": "workspace:*",
    "@cd/data-access": "workspace:*",
    "axios": "1.2.1",
    "body-parser": "1.20.2",
    "cors": "2.8.5",
    "express": "4.18.2",
    "find-up": "5.0.0",
    "stripe": "15.3.0",
    "supertokens-node": "16.6.1"
  },
  "devDependencies": {
    "@babel/cli": "7.20.7",
    "@babel/core": "7.21.4",
    "@babel/node": "7.20.7",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-typescript": "7.23.2",
    "@types/jest": "^29.5.12",
    "@types/node": "20.12.7",
    "babel-plugin-inline-dotenv": "1.7.0",
    "babel-watch": "7.7.2",
    "dotenv": "16.0.3",
    "dotenv-cli": "7.0.0",
    "dotenv-expand": "10.0.0",
    "eslint-plugin-prettier": "5.1.3",
    "jest": "29.7.0",
    "nodemon": "2.0.20",
    "pm2": "^5.2.2",
    "prettier": "3.2.5",
    "rimraf": "4.4.1",
    "supertest": "6.3.3",
    "ts-jest": "^29.1.2",
    "typescript": "5.4.5"
  },
  "main": "./src/index.ts",
  "name": "@cd/server-payments",
  "private": true,
  "scripts": {
    "build": "rimraf ./dist && dotenv -e ../../.env.production -- babel ./src --out-dir dist --extensions \".ts,.tsx\"",
    "clean": "rm -rf ./.next ./out ./dist ./build ./node_modules ./coverage ./tsconfig.tsbuildinfo",
    "build:ci": "dotenv -e ../../.env.production -- babel ./src --out-dir dist --extensions \".ts,.tsx\"",
    "test:unit": "echo \"Tests are not ready for use.\" && exit 0",
    "test:unit-ci": "echo \"Tests are not ready for use.\" && exit 0",
    "test:fix": "../../node_modules/jest/bin/jest.js --detectOpenHandles",
    "test:watch": "../../node_modules/jest/bin/jest.js --watchAll",
    "watch": "rimraf ./node_modules/.cache && dotenv -e ../../.env.development -- babel-watch -e \".ts\" -c ./babel.config.js ./src/index.ts",
    "stripe:webhook": "stripe listen --forward-to localhost:6021/webhook",
    "stripe:webhook-trigger": "stripe trigger checkout.session.completed",
    "start": "dotenv -e ../../.env.production -- node dist"
  },
  "version": "0.0.1"
}
