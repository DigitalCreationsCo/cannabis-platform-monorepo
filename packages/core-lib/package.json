{
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "name": "@cd/core-lib",
  "devDependencies": {
    "@cd/eslint-config": "workspace:^",
    "@cd/ui-lib": "*",
    "@reduxjs/toolkit": "1.9.1",
    "@rollup/plugin-commonjs": "24.0.1",
    "@rollup/plugin-node-resolve": "15.0.1",
    "@rollup/plugin-typescript": "11.0.0",
    "@types/jest": "29.2.4",
    "@types/node": "18.11.10",
    "@types/react": "18.0.34",
    "@types/react-dom": "18.0.11",
    "axios": "1.2.1",
    "cross-env": "7.0.3",
    "dotenv": "16.0.3",
    "esbuild": "0.8.32",
    "eslint": "8.29.0",
    "jest": "29.3.1",
    "jest-environment-jsdom": "29.5.0",
    "microbundle": "0.15.1",
    "prettier": "2.8.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hot-toast": "2.4.0",
    "react-redux": "8.0.2",
    "rimraf": "4.4.1",
    "rollup": "3.20.2",
    "ts-jest": "29.0.3",
    "typescript": "4.9.3"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "private": true,
  "scripts": {
    "build": "rimraf ./dist && cross-env NODE_ENV=production microbundle --tsconfig ./tsconfig.build.json --jsx React.createElement --jsxFragment React.Fragment -f cjs,es --no-compress",
    "build-react17jsx": "microbundle --tsconfig ./tsconfig.build.json --jsx jsx --jsxImportSource react --globals react/jsx-runtime=jsx --compress",
    "clean": "rimraf ./dist ./coverage ./tsconfig.tsbuildinfo",
    "dev": "microbundle watch",
    "lint-fix": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs --fix",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs --cache --cache-location ../../.cache/eslint/core-lib.eslintcache",
    "test": "run-s test-unit",
    "test-unit": "vitest run",
    "test-unit-jest": "jest --config jest.config.js",
    "test-unit-watch": "vitest watch --ui",
    "typecheck": "tsc --project ./tsconfig.json --noEmit"
  },
  "type": "module",
  "types": "dist/index.d.ts",
  "version": "0.0.1"
}
