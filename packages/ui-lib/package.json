{
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "name": "@cd/ui-lib",
  "sideEffects": false,
  "dependencies": {
    "@cd/core-lib": "workspace:*",
    "@cd/data-access": "workspace:*",
    "@cd/eslint-config": "workspace:*",
    "@heroicons/react": "2.1.3",
    "crontab-react": "1.5.3",
    "daisyui": "4.10.2",
    "date-fns-tz": "3.1.3",
    "framer-motion": "10.15.0",
    "i18next": "22.5.1",
    "next-i18next": "13.3.0",
    "react-daisyui": "5.0.0",
    "react-dropzone": "14.2.3",
    "react-hot-toast": "2.4.1",
    "react-multi-carousel": "2.8.5",
    "swr": "2.2.5",
    "tailwind-merge": "1.8.0",
    "tailwindcss": "3.4.3",
    "usehooks-ts": "2.9.1",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@babel/preset-env": "7.23.2",
    "@babel/preset-react": "7.22.15",
    "@babel/preset-typescript": "7.23.2",
    "@types/jest": "29.5.12",
    "@types/node": "20.12.7",
    "@types/react": "18.2.79",
    "@types/react-dom": "18.0.11",
    "autoprefixer": "10.4.19",
    "eslint": "8.57.0",
    "jest": "29.7.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.38",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sync-directory": "6.0.4",
    "tsup": "6.7.0",
    "typescript": "5.4.5"
  },
  "private": true,
  "scripts": {
    "build": "yarn build:css && rm -rf ./dist && tsup src/index.ts",
    "build:ci": "tsup src/index.ts",
    "build:css": "npx tailwindcss -i ./src/styles/ui-lib.css -o ./src/styles/tailwind.css --minify",
    "build:watch": "rm -rf ./dist && tsup --watch",
    "clean": "rm -rf ./.next ./out ./dist ./build ./node_modules ./coverage ./tsconfig.tsbuildinfo",
    "watch": "npm-run-all --parallel --print-label watch:css build:watch",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs --fix",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs --cache --cache-location ../../.cache/eslint/ui-lib.eslintcache",
    "test:unit": "echo \"No unit tests yet\" && exit 0",
    "test:unit-ci": "echo \"No unit tests yet\" && exit 0",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "sync-assets": "rm -rf ./public/assets && syncdir ../../static/assets ./public/assets --copy",
    "watch:css": "npx tailwindcss -i ./src/styles/ui-lib.css -o ./src/styles/tailwind.css --watch"
  },
  "version": "0.8.0"
}
