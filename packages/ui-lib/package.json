  "peerDependencies": {
    "@types/react": "18.0.34",
    "@types/react-dom": "18.0.11",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "@types/react-dom": {
      "optional": true
    }
  },
  "private": true,
  "scripts": {
    "build": "rimraf ./dist && tsup src/index.ts",
    "build:css": "npx tailwindcss -i ./src/styles/index.css -o ./dist/index.css --minify",
    "build:css-watch": "npx tailwindcss -i ./src/styles/index.css -o ./dist/index.css --minify --watch",
    "clean": "rimraf ./dist ./build ./tsconfig.tsbuildinfo ./node_modules/.cache",
    "watch": "rimraf ./dist && tsup --watch",
    "lint-fix": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs --fix",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs --cache --cache-location ../../.cache/eslint/ui-lib.eslintcache",
    "test-unit": "echo \"No unit tests yet\" && exit 0",
    "test-unit-ci": "echo \"No unit tests yet\" && exit 0",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "sync-assets": "rimraf ./public/assets && syncdir ../../static/assets ./public/assets --copy"
  },
  "version": "0.0.1"
}
