	"private": true,
	"scripts": {
		"build": "yarn build:css && rm -rf ./dist && tsup src/index.ts",
		"build:ci": "tsup src/index.ts",
		"build:css": "tailwindcss -i ./src/styles/ui-lib.css -o ./src/styles/tailwind.css",
		"build:watch": "rimraf ./dist && tsup --watch",
		"clean": "rm -rf ./.next ./out ./dist ./build ./node_modules ./coverage ./tsconfig.tsbuildinfo",
		"watch": "npm-run-all --parallel --print-label watch-tailwind build:watch",
		"lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs --fix",
		"lint": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs --cache --cache-location ../../.cache/eslint/ui-lib.eslintcache",
		"test:unit": "echo \"No unit tests yet\" && exit 0",
		"test:unit-ci": "echo \"No unit tests yet\" && exit 0",
		"typecheck": "tsc --project ./tsconfig.json --noEmit",
		"sync-assets": "rimraf ./public/assets && syncdir ../../static/assets ./public/assets --copy",
		"watch-tailwind": "tailwindcss -o ./src/styles/tailwind.css --watch"
	},
	"version": "0.0.1"
