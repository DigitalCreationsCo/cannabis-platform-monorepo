{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Next.js",
    "extends": "./base.json",
    "compilerOptions": {
      "allowJs": true,
      "declaration": false,
      "declarationMap": false,
      "incremental": true,
      "jsx": "react-jsx",
      "lib": ["dom", "dom.iterable", "esnext"],
      "module": "esnext",
      "noEmit": true,
      "resolveJsonModule": true,
      "strict": false,
      "target": "ESNext"
    },
    "include": ["src", "next-env.d.ts"]
  }