	"peerDependenciesMeta": {
		"react": {
			"optional": true
		},
		"react-native": {
			"optional": true
		},
		"@types/react": {
			"optional": true
		},
		"@types/react-dom": {
			"optional": true
		},
		"@types/react-native": {
			"optional": true
		}
	},
	"private": true,
	"scripts": {
		"build": "yarn build:css && rm -rf ./dist && tsup src/index.ts",
		"build:ci": "tsup src/index.ts",
		"build:storybook": "storybook build --output-dir dist/storybook",
		"clean": "rimraf ./.next ./out ./dist ./build ./node_modules ./coverage ./tsconfig.tsbuildinfo ./node_modules/.cache",
		"watch": "rimraf ./dist && tsup --watch",
		"lint-fix": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs --fix",
		"lint": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs --cache --cache-location ../../.cache/eslint/native-ui.eslintcache",
		"test:unit": "echo \"No unit tests yet\" && exit 0",
		"test:unit-ci": "echo \"No unit tests yet\" && exit 0",
		"typecheck": "tsc --project ./tsconfig.json --noEmit",
		"storybook": "storybook dev -p 6006",
		"sync-assets": "rimraf ./public/assets && syncdir ../../static/assets ./public/assets --copy",
		"build-storybook": "storybook build"
	},
	"version": "0.0.1"
}
