{
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "name": "@gras/core",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./auth": {
      "import": "./dist/auth.js",
      "require": "./dist/auth.cjs",
      "types": "./dist/auth.d.ts"
    },
    "./constants": {
      "import": "./dist/constants.js",
      "require": "./dist/constants.cjs",
      "types": "./dist/constants.d.ts"
    },
    "./crm": {
      "import": "./dist/crm.js",
      "require": "./dist/crm.cjs",
      "types": "./dist/crm.d.ts"
    },
    "./cron-job": {
      "import": "./dist/cron-job.js",
      "require": "./dist/cron-job.cjs",
      "types": "./dist/cron-job.d.ts"
    },
    "./hooks": {
      "import": "./dist/hooks.js",
      "require": "./dist/hooks.cjs",
      "types": "./dist/hooks.d.ts"
    },
    "./lib": {
      "import": "./dist/lib.js",
      "require": "./dist/lib.cjs",
      "types": "./dist/lib.d.ts"
    },
    "./locales": {
      "import": "./dist/locales.js",
      "require": "./dist/locales.cjs",
      "types": "./dist/locales.d.ts"
    },
    "./middleware": {
      "import": "./dist/middleware.js",
      "require": "./dist/middleware.cjs",
      "types": "./dist/middleware.d.ts"
    },
    "./point-of-sale/*": {
      "import": "./dist/point-of-sale.js",
      "require": "./dist/point-of-sale.cjs",
      "types": "./dist/point-of-sale.d.ts"
    },
    "./reducer": {
      "import": "./dist/reducer.js",
      "require": "./dist/reducer.cjs",
      "types": "./dist/reducer.d.ts"
    },
    "./seo": {
      "import": "./dist/seo.js",
      "require": "./dist/seo.cjs",
      "types": "./dist/seo.d.ts"
    },
    "./sms/*": {
      "import": "./dist/sms.js",
      "require": "./dist/sms.cjs",
      "types": "./dist/sms.d.ts"
    },
    "./types": {
      "import": "./dist/types.js",
      "require": "./dist/types.cjs",
      "types": "./dist/types.d.ts"
    },
    "./utils": {
      "import": "./dist/utils.js",
      "require": "./dist/utils.cjs",
      "types": "./dist/utils.d.ts"
    }
  },
  "dependencies": {
    "@gras/data-access": "workspace:*",
    "@gras/eslint-config": "workspace:*",
    "@paralleldrive/cuid2": "2.2.1",
    "@reduxjs/toolkit": "2.2.1",
    "api": "6.1.1",
    "axios": "1.2.1",
    "crypto-js": "4.2.0",
    "date-fns-tz": "3.1.3",
    "dotenv": "16.0.3",
    "dotenv-expand": "10.0.0",
    "find-up": "5.0.0",
    "haversine-distance": "1.2.1",
    "react": "18.3.1",
    "react-cookie": "4.1.1",
    "react-dom": "18.3.1",
    "react-redux": "8.0.2",
    "socket.io-client": "4.7.4",
    "swr": "2.2.5",
    "twilio": "5.0.3",
    "typescript": "5.4.5"
  },
  "devDependencies": {
    "@types/crypto-js": "4.2.2",
    "@types/react": "18.2.79",
    "@types/react-dom": "18.0.11",
    "@types/socket.io-client": "3.0.0",
    "@vitejs/plugin-react": "4.3.1",
    "eslint": "8.57.0",
    "jest": "29.7.0",
    "jest-junit": "16.0.0",
    "tsup": "6.7.0",
    "vite-tsconfig-paths": "4.0.7",
    "vitest": "1.6.0"
  },
  "private": true,
  "scripts": {
    "build": "rm -rf ./dist && tsup",
    "build:ci": "rm -rf ./dist && tsup",
    "clean": "rm -rf ./.next ./out ./dist ./build ./node_modules ./coverage ./tsconfig.tsbuildinfo",
    "watch": "tsup --watch",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs --fix",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs --cache --cache-location ../../.cache/eslint/core.eslintcache",
    "test": "vitest run",
    "test:ci": "vitest run --reporter=junit",
    "test:watch": "vitest watch --ui",
    "typecheck": "tsc --project ./tsconfig.json --noEmit"
  },
  "version": "0.0.1"
}
