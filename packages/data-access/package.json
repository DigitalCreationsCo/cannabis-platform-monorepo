{
  "dependencies": {
    "@cd/shared-config": "*",
    "@prisma/client": "^4.4.0"
  },
  "devDependencies": {
    "@types/jest": "29.2.4",
    "concurrently": "7.6.0",
    "dotenv-cli": "7.0.0",
    "jest": "29.3.1",
    "jest-mock-extended": "3.0.1",
    "prisma": "^4.4.0",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "typescript": "4.9.3"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "name": "@cd/data-access",
  "prisma": {
    "schema": "./schema.prisma",
    "seed": "ts-node --esm src/db/seed.ts"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "private": true,
  "scripts": {
    "build": "tsc -b",
    "build:prod": "dotenv -e ../../.env.production -- prisma migrate deploy && tsc -b",
    "db:clear": "ts-node --esm src/db/clear.ts",
    "db:generate": "npx prisma generate",
    "db:migrate-init": "dotenv -e ../../.env.development -- prisma migrate dev --name initial-migration --create-only && prisma migrate dev",
    "db:migrate": "dotenv -e ../../.env.development -- prisma migrate dev",
    "db:migrate-reset": "dotenv -e ../../.env.development -- prisma migrate reset",
    "db:migrate-prod": "dotenv -e ../../.env.production -- prisma migrate deploy",
    "db:push": "npx prisma db push",
    "db:push-force": "npx prisma db push --force-reset --accept-data-loss",
    "db:reseed": "npx prisma db push && prisma generate && yarn db:clear && prisma db seed && prisma generate && yarn build",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "test": "../../node_modules/jest/bin/jest.js",
    "watch": "tsc --watch"
  },
  "types": "dist/index.d.ts",
  "version": "0.0.1"
}
