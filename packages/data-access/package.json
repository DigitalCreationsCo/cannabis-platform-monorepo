{
  "name": "@cd/data-access",
  "version": "0.0.1",
  "private": true,
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "prisma": {
    "seed": "ts-node --esm src/db/seed.ts"
  },
  "scripts": {
    "dev": "tsc --watch src/index.ts",
    "test": "../../node_modules/jest/bin/jest.js",
    "generate": "prisma generate",
    "connect": "pscale connect cannabis_delivery_v1 dev --port 3309",
    "migrate": "prisma migrate",
    "push": "prisma db push",
    "deploy": "pscale deploy-request create cannabis_delivery_v1 dev",
    "seed": "prisma db seed",
    "clear": "ts-node --esm src/db/clear.ts",
    "reseed": "prisma generate && prisma db push && yarn db clear && prisma db seed",
    "studio": "prisma studio"
  },
  "peerDependencies": {
    "@cd/shared-config": "*",
    "@prisma/client": "4.4.0",
    "@types/jest": "29.2.4",
    "jest": "29.3.1",
    "jest-mock-extended": "3.0.1",
    "prisma": "4.4.0",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "typescript": "4.9.3"
  }
}
