{
  "sideEffects": false,
  "devDependencies": {
    "axios": "1.2.1",
    "dotenv-cli": "7.0.0",
    "eslint": "^9.15.0",
    "tsup": "6.7.0",
    "typescript": "5.5.2"
  },
  "main": "./src/index.ts",
  "name": "@gras/data-access",
  "exports": {
    ".": "./src/index.ts"
  },
  "private": true,
  "scripts": {
    "build": "rm -rf ./dist && tsup",
    "build:ci": "rm -rf ./dist && tsup",
    "clean": "rm -rf ./dist ./coverage ./tsconfig.tsbuildinfo",
    "watch": "rm -rf ./dist && tsup --watch",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs --fix",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs --cache --cache-location ../../.cache/eslint/db-main-prisma.eslintcache",
    "shadow-db": "sh ../../scripts/docker/shadow-db.sh",
    "seed": "npx prisma generate && dotenv -e ../../.env.development -- yarn prisma db seed",
    "seed-production": "npx prisma generate && dotenv -e ../../.env.production -- yarn prisma db seed",
    "debug:seed": "yarn clear-tables && yarn prisma generate && yarn push && dotenv -e ../../.env.development -- node --import ./register.mjs ./src/db/seed.mts",
    "baseline": "dotenv -e ../../.env.production -- sh scripts/create-baseline.sh",
    "clear-tables": "dotenv -e ../../.env.development -- node --import ./register.mjs ./src/db/clear.mts",
    "clear-tables-production-yes-im-sure": "dotenv -e ../../.env.production -- node --import ./register.mjs ./src/db/clear.mts",
    "run-prod": "dotenv -e ../../.env.production -- node $0",
    "test": "run-s test:unit",
    "test:unit": "echo \"No unit tests configured\" && exit 0",
    "test:unit-ci": "echo \"No unit tests configured\" && exit 0",
    "test:unit-coverage": "echo \"No unit tests configured\" && exit 0",
    "typecheck": "tsc --project ./tsconfig.json --noEmit"
  },
  "version": "0.0.1",
  "dependencies": {
    "@gras/eslint-config": "workspace:*",
    "@paralleldrive/cuid2": "2.2.1",
    "mongodb": "5.8.1"
  }
}
