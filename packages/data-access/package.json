{
  "dependencies": {
    "@cd/shared-config": "*",
    "@prisma/client": "^4.4.0"
  },
  "devDependencies": {
    "@types/jest": "29.2.4",
    "concurrently": "7.6.0",
    "dotenv-cli": "7.0.0",
    "jest": "29.3.1",
    "jest-mock-extended": "3.0.1",
    "prisma": "^4.4.0",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "typescript": "4.9.3"
  },
  "files": [
    "dist"
  ],
  "main": "./dist",
  "name": "@cd/data-access",
  "prisma": {
    "schema": "./schema.prisma",
    "seed": "ts-node --esm src/db/seed.ts"
  },
  "private": true,
  "scripts": {
    "build": "tsc -b",
    "build:dev": "tsc -b",
    "build:prod": "dotenv -e ../../.env.production -- prisma migrate deploy && tsc -b",
    "db:clear": "ts-node --esm src/db/clear.ts",
    "db:connect": "pscale connect cannabis_delivery_v1 dev --port 3309",
    "db:deploy": "pscale deploy-request create cannabis_delivery_v1 dev",
    "db:generate": "prisma generate",
    "db:migrate-init": "prisma migrate dev --name initial-migration --create-only && prisma migrate dev",
    "db:migrate-init2": "dotenv -e ../../.env.development -- prisma migrate dev --name initial-migration --create-only && prisma migrate dev",
    "db:migrate-dev": "dotenv -e ../../.env.development -- prisma migrate dev",
    "db:migrate-prod": "dotenv -e ../../.env.production -- prisma migrate deploy",
    "db:push": "prisma db push --skip-generate",
    "db:reseed": "prisma db push && prisma generate && yarn db:clear && prisma db seed && prisma generate && yarn build",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "test": "../../node_modules/jest/bin/jest.js",
    "watch": "concurrently \"yarn watch:ts\"",
    "watch:ts": "tsc --watch"
  },
  "version": "0.0.1"
}
