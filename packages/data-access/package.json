{
  "peerDependencies": {
    "axios": "1.2.1"
  },
  "peerDependenciesMeta": {
    "axios": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/core": "7.21.4",
    "@babel/node": "7.20.7",
    "@cd/eslint-config": "workspace:*",
    "@testing-library/jest-dom": "5.16.5",
    "@types/jest": "29.2.4",
    "@types/node": "18.11.10",
    "@types/react": "18.0.34",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "5.45.0",
    "@typescript-eslint/parser": "5.45.0",
    "axios": "1.2.1",
    "cross-env": "7.0.3",
    "dotenv-cli": "7.0.0",
    "dotenv-flow-cli": "1.0.0",
    "eslint": "8.37.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "jest": "29.3.1",
    "jest-junit": "16.0.0",
    "jest-mock-extended": "3.0.1",
    "mongodb": "5.8.1",
    "prettier": "2.8.0",
    "prisma": "4.14.0",
    "rimraf": "4.4.1",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "tsup": "6.7.0",
    "tsx": "3.12.6",
    "typescript": "4.9.3"
  },
  "main": "./dist/index.js",
  "name": "@cd/data-access",
  "prisma": {
    "seed": "node --loader ts-node/esm ./src/db/seed.mts",
    "schema": "./prisma/schema.prisma",
    "generate": "npx prisma generate",
    "clear-db": "node --loader ts-node/esm ./src/db/clear.mts"
  },
  "private": true,
  "scripts": {
    "build": "rimraf ./dist && tsup",
    "build:ci": "rm -rf ./dist && tsup",
    "clean": "rm -rf ./dist ./coverage ./tsconfig.tsbuildinfo",
    "debug-test-e2e": "cross-env DEBUG=testcontainers vitest run $@",
    "watch": "rimraf ./dist && tsup --watch",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs --fix",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs --cache --cache-location ../../.cache/eslint/db-main-prisma.eslintcache",
    "postinstall": "chmod +x scripts/* ../../scripts/* && npx prisma generate",
    "studio": "dotenv -e ../../.env.development -- yarn prisma studio",
    "studio-production": "dotenv -e ../../.env.production -- yarn prisma studio",
    "shadow-db": "../../scripts/docker/shadow-db.sh",
    "push": "dotenv -e ../../.env.development -- yarn prisma db push",
    "pull": "dotenv -e ../../.env.development -- npx prisma db pull",
    "seed": "npx prisma generate && dotenv -e ../../.env.development -- yarn prisma db seed",
    "seed-production": "npx prisma generate && dotenv -e ../../.env.production -- yarn prisma db seed",
    "reseed": "yarn clear-db && yarn seed",
    "clear-tables": "dotenv -e ../../.env.development -- node --loader ts-node/esm ./src/db/clear.mts",
    "clear-tables-production-yes-im-sure": "dotenv -e ../../.env.production -- node --loader ts-node/esm ./src/db/clear.mts",
    "baseline": "dotenv -e ../../.env.production -- scripts/create-baseline",
    "migrate-dev": "scripts/migrate-dev",
    "migrate-deploy": "dotenv -e ../../.env.production -- npx prisma migrate deploy",
    "migrate-diff": "yarn dotenv -e ../../.env.production -- scripts/migrate-diff",
    "migrate-resolve": "dotenv -e ../../.env.production -- prisma migrate resolve --applied",
    "migrate-rollback": "dotenv -e ../../.env.production -- yarn prisma migrate resolve --rolled-back",
    "migrate-revert": "yarn dotenv -e ../../.env.production -- scripts/migrate-revert",
    "migrate-reset-dev": "dotenv -e ../../.env.development -- yarn prisma migrate reset",
    "migrate-reset-production": "dotenv -e ../../.env.production -- yarn prisma migrate reset",
    "migrate-status": "dotenv -e ../../.env.production -- yarn prisma migrate status",
    "run-prod": "dotenv -e ../../.env.production -- node $0",
    "test": "run-s test:unit",
    "test:e2e": "dotenv -e ../../.env.development -- yarn vitest run",
    "test:unit": "jest --config ./jest.config.ts",
    "test:unit-ci": "jest --config ./jest.config.ts --ci --runInBand --reporters=default --reporters=jest-junit",
    "test:unit-coverage": "yarn test:unit --collect-coverage=true",
    "typecheck": "tsc --project ./tsconfig.json --noEmit"
  },
  "version": "0.0.1",
  "dependencies": {
    "@paralleldrive/cuid2": "2.2.1",
    "@prisma/client": "4.14.0"
  }
}
