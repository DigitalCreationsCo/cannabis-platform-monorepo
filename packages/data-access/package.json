{
  "devDependencies": {
    "@cd/eslint-config": "workspace:*",
    "@testing-library/jest-dom": "5.16.5",
    "@types/jest": "29.2.4",
    "@types/node": "18.11.10",
    "@types/react": "18.0.34",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "5.45.0",
    "@typescript-eslint/parser": "5.45.0",
    "cross-env": "7.0.3",
    "dotenv-cli": "7.0.0",
    "dotenv-flow-cli": "1.0.0",
    "eslint": "8.29.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "jest": "29.3.1",
    "jest-mock-extended": "3.0.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.0",
    "prisma": "4.10.0",
    "rimraf": "4.4.1",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "tsup": "6.7.0",
    "tsx": "3.12.6",
    "typescript": "4.9.3"
  },
  "dependencies": {
    "@prisma/client": "4.10.0"
  },
  "main": "./dist/index.js",
  "name": "@cd/data-access",
  "prisma": {
    "seed": "node --loader ts-node/esm ./src/db/seed.mts",
    "schema": "./prisma/schema.prisma",
    "generate": "prisma generate"
  },
  "private": true,
  "scripts": {
    "build": "rimraf ./dist && tsup",
    "clean": "rimraf ./dist ./coverage ./tsconfig.tsbuildinfo",
    "debug-test-e2e": "cross-env DEBUG=testcontainers vitest run $@",
    "watch": "rimraf ./dist && tsup --watch",
    "fix-all-files": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs --fix",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs --cache --cache-location ../../.cache/eslint/db-main-prisma.eslintcache",
    "postinstall": "prisma generate",
    "db-clear": "dotenv -e ../../.env.development -- node --loader ts-node/esm ./src/db/clear.mts",
    "db-push": "dotenv -e ../../.env.development -- yarn prisma db push",
    "db-seed": "npx prisma generate && dotenv -e ../../.env.development -- yarn prisma db seed",
    "db-migrate": "dotenv -e ../../.env.development  -- yarn prisma migrate dev",
    "db-migrate-reset": "dotenv -e ../../.env.development -- yarn prisma migrate reset",
    "prisma-studio": "dotenv -e ../../.env.development -- yarn prisma studio",
    "test": "run-s test:unit",
    "test-e2e": "dotenv -e ../../.env.development -- yarn vitest run",
    "test-unit": "jest --config ./jest.config.ts",
    "test-unit-coverage": "yarn test-unit --collect-coverage=true",
    "typecheck": "tsc --project ./tsconfig.json --noEmit"
  },
  "version": "0.0.1"
}
