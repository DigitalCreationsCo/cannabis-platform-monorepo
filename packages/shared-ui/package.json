{
  "exports": {
    ".": "./dist/index.js",
    "./dist/style/index.css": "./dist/style/index.css"
  },
  "main": "dist/index.js",
  "name": "@cd/shared-ui",
  "peerDependencies": {
    "@deanc/esbuild-plugin-postcss": "1.0.2",
    "@rollup/plugin-commonjs": "24.0.1",
    "@rollup/plugin-image": "3.0.2",
    "@rollup/plugin-node-resolve": "15.0.1",
    "@rollup/plugin-typescript": "11.0.0",
    "@types/react": "18.0.34",
    "@types/react-dom": "18.0.11",
    "babel-loader": "9.1.2",
    "css-loader": "6.7.3",
    "esbuild": "0.17.15",
    "esbuild-plugin-inline-image": "0.0.9",
    "file-loader": "6.2.0",
    "postcss": "8.4.19",
    "prettier": "2.8.0",
    "rollup": "3.20.2",
    "rollup-plugin-postcss": "4.0.2",
    "style-loader": "3.3.2",
    "ts-loader": "9.4.2",
    "webpack": "5.78.0",
    "webpack-cli": "5.0.1",
    "webpack-dev-server": "4.13.2",
    "yup": "0.32.11"
  },
  "private": true,
  "scripts": {
    "build:clean": "rm -rf dist",
    "build": "yarn build:clean && ../../node_modules/webpack/bin/webpack.js --mode production",
    "esbuild": "yarn build:clean && node ../../node_modules/typescript/bin/tsc && node esbuild.js && yarn build:css",
    "build:css": "npx tailwindcss -i ./src/index.css -o ./dist/style/index.css --minify",
    "build:css-watch": "npx tailwindcss -i ./src/index.css -o ./dist/style/index.css --minify --watch",
    "rollup": "NODE_ENV=production rollup -c",
    "esbuild-watch": "yarn build:clean && node ../../node_modules/typescript/bin/tsc && node esbuild.watch.js && yarn build:css-watch"
  },
  "types": "dist/index.d.ts",
  "type": "module",
  "version": "0.0.1"
}
