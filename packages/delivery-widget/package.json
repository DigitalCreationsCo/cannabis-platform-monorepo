{
  "name": "@cd/delivery-widget",
  "private": true,
  "version": "0.0.1",
  "main": "./dist",
  "scripts": {
    "build": "yarn build:clean && tsc",
    "build:clean": "rm -rf dist",
    "build:watch": "tsc --watch",
    "bundle:widget": "tsc src/* --out /dev/stdout | browserify -o ./dist/widget.js",
    "bundle:ejs": "copyfiles -u 3 \"./src/**/*.ejs\" \"./dist/widget/views\" --verbose",
    "build:react": "webpack",
    "start": "yarn build && node dist",
    "react:dev": "webpack serve",
    "serve:dev": "concurrently \"yarn build:watch\" \"yarn serve:js\" \"yarn serve:hostpage\"",
    "serve:hostpage": "browser-sync start -s \"public\" -f . --port 4040 --ui-port 4041 --cors",
    "serve:js": "browser-sync start -s \"dist\" --port 4050 --ui-port 4051 --cors",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "peerDependencies": {
    "@babel/core": "7.21.4",
    "@babel/preset-env": "7.21.4",
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "7.18.6",
    "@cd/shared-ui": "*",
    "@formkit/auto-animate": "1.0.0-beta.6",
    "@types/ejs": "3.1.2",
    "@types/react": "18.0.34",
    "@types/react-dom": "18.0.11",
    "babel-loader": "9.1.2",
    "browser-sync": "2.29.1",
    "browserify": "17.0.0",
    "cheerio": "1.0.0-rc.12",
    "concurrently": "8.0.1",
    "copyfiles": "2.4.1",
    "css-loader": "6.7.3",
    "ejs": "3.1.9",
    "express": "4.18.2",
    "file-loader": "6.2.0",
    "postcss": "8.4.19",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.10.0",
    "serve": "14.2.0",
    "style-loader": "3.3.2",
    "tailwind-merge": "1.8.0",
    "tailwindcss": "3.2.4",
    "ts-loader": "9.4.2",
    "typescript": "4.9.3",
    "webpack": "5.78.0",
    "webpack-cli": "5.0.1",
    "webpack-dev-server": "4.13.2"
  },
  "type": "module"
}
