{
  "name": "@gras/ui",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "sideEffects": false,
  "dependencies": {
    "@calcom/embed-react": "^1.5.2",
    "@gras/core": "workspace:*",
    "@gras/data-access": "workspace:*",
    "@gras/eslint-config": "workspace:*",
    "@mantine/carousel": "^7.16.0",
    "@mantine/core": "7.16.0",
    "@mantine/hooks": "7.16.0",
    "@tabler/icons-react": "^3.6.0",
    "cookies-next": "^5.1.0",
    "crontab-react": "1.5.3",
    "daisyui": "4.10.2",
    "date-fns-tz": "3.1.3",
    "framer-motion": "10.15.0",
    "i18next": "22.5.1",
    "next-intl": "^3.26.3",
    "react-daisyui": "5.0.0",
    "react-dropzone": "14.2.3",
    "react-google-recaptcha": "^3.1.0",
    "react-hot-toast": "2.4.1",
    "react-multi-carousel": "2.8.5",
    "swr": "2.2.5",
    "tailwind-merge": "1.8.0",
    "tailwindcss": "3.4.3",
    "usehooks-ts": "3.1.0",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "7.23.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.8",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.1",
    "autoprefixer": "10.4.19",
    "cross-env": "^7.0.3",
    "eslint": "^9.15.0",
    "jest": "^29.7.0",
    "npm-run-all": "4.1.5",
    "postcss": "^8.4.38",
    "postcss-preset-mantine": "1.17.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sync-directory": "6.0.4",
    "tsup": "6.7.0",
    "typescript": "5.5.2"
  },
  "private": true,
  "scripts": {
    "build": "rm -rf ./dist && cross-env NODE_ENV=production yarn build:css && cross-env NODE_ENV=production tsup src/index.ts",
    "build:ci": "cross-env NODE_ENV=production yarn build:css && cross-env NODE_ENV=production tsup src/index.ts",
    "build:css": "npm run copy-shared-assets && npx tailwindcss -i ./styles/globals.css -o ./styles/tailwind.css --minify",
    "build:watch": "rm -rf ./dist && tsup --watch",
    "copy-shared-assets": "sh ../../scripts/copysharedCSSandAssets.sh",
    "clean": "rm -rf ./.next ./out ./dist ./build ./node_modules ./coverage ./tsconfig.tsbuildinfo",
    "watch": "npm-run-all --parallel --print-label watch:css build:watch",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs --fix",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs --cache --cache-location ../../.cache/eslint/ui.eslintcache",
    "test:unit": "echo \"No unit tests yet\" && exit 0",
    "test:unit-ci": "echo \"No unit tests yet\" && exit 0",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "sync-assets": "rm -rf ./public/assets && syncdir ../../static/assets ./public/assets --copy",
    "watch:css": "npx tailwindcss -i ./styles/globals.css -o ./styles/tailwind.css --minify --watch",
    "watch:css-bundle": "cross-env NODE_ENV=production npx tailwindcss -i ./styles/globals.css -o ./styles/tailwind.css --minify --watch"
  },
  "version": "0.8.1"
}
