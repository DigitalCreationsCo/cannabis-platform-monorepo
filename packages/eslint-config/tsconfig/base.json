{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "Default",
	"compilerOptions": {
		"declaration": true,
		"forceConsistentCasingInFileNames": true,
		"moduleResolution": "node",
		"module": "ESNext",
		"noUnusedLocals": false,
		"noUnusedParameters": false,
		"preserveWatchOutput": true,
		"strict": false,
		"target": "ESNext",
		"lib": ["ESNext", "dom", "dom.iterable"]
	}
}
