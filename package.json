		"babel-loader": "9.1.2",
		"babel-plugin-css-modules-transform": "1.6.2",
		"babel-plugin-inline-dotenv": "1.7.0",
		"babel-watch": "7.7.2",
		"bcryptjs": "2.4.3",
		"browser-sync": "2.29.1",
		"browserify": "17.0.0",
		"busboy": "1.6.0",
		"cheerio": "1.0.0-rc.12",
		"chmod": "0.2.1",
