  },
  "resolutions": {
    "typescript": "5.4.5",
    "@types/react": "18.2.79",
    "@types/react-dom": "18.0.11",
    "postcss": "8.4.38",
    "postcss-js": "4.0.1"
  },
  "dependencies": {
    "global": "^4.4.0",
    "storybook-addon-performance": "^0.17.3"
  }
}
