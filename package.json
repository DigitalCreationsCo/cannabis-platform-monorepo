{
  "name": "cannabis-platform-monorepo",
  "version": "0.0.1",
  "private": true,
  "keywords": [
    "monorepo",
    "Next.js",
    "Sanity",
    "mongo",
    "twilio"
  ],
  "homepage": "https://github.com/DigitalCreationsCo/cannabis-platform-monorepo",
  "repository": {
    "type": "git",
    "url": "DigitalCreationsCo/cannabis-platform-monorepo"
  },
  "workspaces": {
    "packages": [
      "app/*",
      "packages/*",
      "server/*",
      "mobile/*",
      "tools/*",
      "twilio/*"
    ]
  },
  "scripts": {
    "g:test-unit": "yarn workspaces foreach -ptv run test:unit",
    "g:test-watch": "cd scripts && node watch/test-watch.js",
    "g:test-unit-changed": "yarn workspaces foreach --since=HEAD~1 run test:unit",
    "g:test-unit-ci": "yarn workspaces foreach --since=origin/master run test:unit-ci",
    "g:test-e2e": "yarn workspaces foreach --since=HEAD~1 run test:e2e",
    "g:build": "NEXT_PUBLIC_IS_LOCAL_BUILD=1 SKIP_BUILD_STATIC_GENERATION=1 yarn workspaces foreach -p --topological-dev -v run build",
    "g:build-all": "yarn workspaces foreach -ptv --include '@gras/*' run build",
    "g:build-changed": "yarn workspaces foreach --topological-dev -v --since=origin/production run build:ci",
    "g:build-ci": "yarn workspaces foreach --topological-dev -v --since=origin/production run build:ci",
    "g:build-css": "yarn workspaces foreach run build:css",
    "g:changeset": "changeset",
    "g:check-dist": "yarn workspaces foreach -ptv run check-dist",
    "g:clean": "yarn workspaces foreach run clean && rm -rf node_modules",
    "g:codegen": "yarn workspaces foreach -ptv run codegen",
    "g:typecheck": "yarn workspaces foreach -ptv run typecheck",
    "g:lint": "yarn workspaces foreach -ptv run lint --color",
    "g:lint-fix": "yarn workspaces foreach -ptv run lint:fix",
    "g:lint-staged-files": "lint-staged --allow-empty",
    "g:lint-styles": "yarn workspaces foreach -ptv run lint-styles --color",
    "g:lint-all": "prettier --write .",
    "g:release": "yarn g:build && changeset publish",
    "g:share-static-hardlink": "yarn workspaces foreach -pv --include '@gras/app-*' run share-static-hardlink",
    "g:share-static-symlink": "yarn workspaces foreach -pv --include '@gras/app-*' run share-static-symlink",
    "packages:build": "yarn workspaces foreach -itR --from packages/* run build",
    "packages:watch": "cd scripts && node watch/packages-watch.js",
    "packages:clean": "yarn workspaces foreach -ptv --include '@gras/*' run clean",
    "packages:lint": "yarn workspaces foreach -ptv --include '@gras/*' run lint",
    "packages:test:unit": "yarn workspaces foreach -ptv --include '@gras/*' run test:unit",
    "packages:typecheck": "yarn workspaces foreach -ptv --include '@gras/*' run typecheck",
    "server:build": "yarn workspaces foreach -itR --from @gras/server-$0 run build",
    "server:build-all": "yarn workspaces foreach -ptv --include '@gras/server-*' run build",
    "server:watch": "cd scripts && node watch/server-watch.js",
    "server:clean": "yarn workspaces foreach -ptv --include '@gras/server-*' run clean",
    "app:dev": "cd scripts && node watch/app-watch.js",
    "app:build": "yarn workspaces foreach -itR --from @gras/app-$0 run build",
    "app:build-all": "yarn workspaces foreach -ptv --include '@gras/app-*' run build",
    "app:clean": "yarn workspaces foreach -ptv --include '@gras/app-*' run clean",
    "dev-all": "concurrently \"yarn packages:watch\" \"yarn server:watch\" \"yarn app:dev\"",
    "autogpt": "cd AutoGPT && docker compose run --service-ports --rm auto-gpt serve",
    "docker:dev": "docker compose -f docker-compose.dev.yml up -d",
    "docker:supertokens": "sh scripts/supertokens-dev.sh",
    "docker:shadow-db": "sh scripts/docker-shadow-db.sh",
    "docker:redis": "sh scripts/docker-redis.sh $0",
    "docker:checkout-widget-demo": "sh scripts/docker/launch-checkout-widget-demo.sh",
    "docker:build": "TAG=\"latest\" BUILD_TYPE=$0 BUILD_CONTEXT=$1 PORT=$2 make docker-build-nocache",
    "docker:build-output": "TAG=\"latest\" BUILD_TYPE=$0 BUILD_CONTEXT=$1 PORT=$2 make docker-build-nocache-output",
    "deploy:app": "IMAGE=$0 PORT=$1 TAG=${TAG:-latest} make deploy-app",
    "deploy:supertokens": "sh scripts/deploy-supertokens.sh",
    "enable-gcloud-services": "sh scripts/enable-gcloud-services.sh",
    "tf:init": "make init",
    "tf:plan": "make plan",
    "tf:apply": "make apply",
    "tf:close": "make destroy",
    "tf:console": "make console",
    "create-update-cert": "sh terraform/scripts/cert-scripts/create-update-cert.sh",
    "update-ssl-cert": "sh terraform/scripts/cert-scripts/update-ssl-cert.sh",
    "cluster:start": "make apply && APPLY_FILE=k8s/ make deploy",
    "cluster:nodes": "kubectl get nodes",
    "cluster:apply": "APPLY_FILE=$0 make deploy",
    "cluster:prune": "kubectl get pods --all-namespaces | grep $0 | awk '{print $2 \" --namespace=\" $1}' | xargs kubectl delete pod",
    "cluster:get-proxy": "sh scripts/get-proxy-config.sh",
    "install:playwright": "playwright install",
    "postinstall": "yarn is-ci || yarn is-docker || yarn husky install && chmod +x scripts",
    "permit-scripts": "node scripts/permit-scripts.js",
    "ml-csv": "sh scripts/generate-annotations-csv.sh",
    "sms:activate": "dotenv -e .env.production -- node server/dispatch/src/lib/sms/activate-sms.mjs",
    "sms:get-phone-numbers": "dotenv -e .env.production -- node server/dispatch/src/lib/sms/get-active-phonenumbers.mjs",
    "read-csr": "openssl req -text -in $0 -noout",
    "read-cert": "openssl x509 -in $0 -text -noout",
    "deps:sync": "yarn syncpack fix-mismatches",
    "snippet": "node scripts/create-snippet.js",
    "build:storybook": "storybook build --output-dir dist/storybook",
    "storybook": "storybook dev -p 6006",
    "storybook debug": "storybook dev -p 6006 --debug-webpack",
    "storybook check": "BABEL_SHOW_CONFIG_FOR=.storybook/main.ts yarn storybook"
  },
  "engines": {
    "node": "18.19.0",
    "yarn": ">=1.22.0",
    "npm": "please-use-yarn"
  },
  "packageManager": "yarn@3.3.1",
  "devDependencies": {
    "@babel/cli": "7.20.7",
    "@babel/core": "7.21.4",
    "@babel/node": "7.20.7",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "7.23.2",
    "@commitlint/cli": "17.6.5",
    "@commitlint/config-conventional": "17.6.5",
    "@formkit/auto-animate": "1.0.0-beta.6",
    "@gras/eslint-config": "workspace:*",
    "@next/bundle-analyzer": "14.2.5",
    "@paralleldrive/cuid2": "2.2.1",
    "@reduxjs/toolkit": "2.2.1",
    "@sentry/nextjs": "8.25.0",
    "@storybook/addon-essentials": "8.1.5",
    "@storybook/addon-interactions": "8.1.5",
    "@storybook/addon-links": "8.1.5",
    "@storybook/addon-onboarding": "8.1.5",
    "@storybook/addon-styling-webpack": "1.0.0",
    "@storybook/blocks": "8.1.5",
    "@storybook/nextjs": "8.1.5",
    "@storybook/react": "8.1.5",
    "@storybook/testing-library": "0.2.2",
    "@tailwindcss/typography": "0.5.12",
    "@testing-library/react": "14.0.0",
    "@types/formidable": "2.0.5",
    "@types/node": "20.12.7",
    "@types/prettier": "2.7.2",
    "@types/react-dom": "18.0.11",
    "@types/recluster": "0.4.3",
    "autoprefixer": "10.4.19",
    "axios": "1.2.1",
    "babel-loader": "9.1.2",
    "babel-plugin-inline-dotenv": "1.7.0",
    "babel-watch": "7.7.2",
    "busboy": "1.6.0",
    "cheerio": "1.0.0-rc.12",
    "chmod": "0.2.1",
    "concurrently": "8.0.1",
    "copyfiles": "2.4.1",
    "cors": "2.8.5",
    "cross-env": "^7.0.3",
    "date-fns": "^3.6.0",
    "date-fns-tz": "3.1.3",
    "dotenv": "16.0.3",
    "dotenv-cli": "7.0.0",
    "dotenv-expand": "10.0.0",
    "dotenv-flow-cli": "1.0.0",
    "dts-bundle": "0.7.3",
    "eslint": "8.57.0",
    "express": "4.18.2",
    "file-loader": "6.2.0",
    "find-up": "5.0.0",
    "formidable": "2.1.1",
    "formik": "2.4.5",
    "fs-extra": "^11.2.0",
    "get-tsconfig": "4.7.2",
    "globby": "11.0.1",
    "husky": "8.0.0",
    "is-ci": "3.0.1",
    "is-docker": "3.0.0",
    "lint-staged": "13.2.0",
    "mongodb": "5.8.1",
    "nanoid": "3.3.4",
    "newrelic": "11.19.0",
    "next": "14.1.0",
    "next-connect": "0.13.0",
    "next-seo": "6.5.0",
