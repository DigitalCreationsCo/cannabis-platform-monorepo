{
  "name": "cannabis-platform-monorepo",
  "version": "0.0.1",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "yarn workspaces foreach --topological-dev --parallel --exclude cannabis-platform-monorepo run build",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "install": "turbo run install",
    "dev": "turbo run dev",
    "app": "yarn workspace app",
    "cfg": "yarn workspace shared-config",
    "ui": "yarn workspace shared-ui",
    "build:app": "yarn workspace app build",
    "docker:app": "scripts/docker/dev-app.sh",
    "permit-scripts": "node scripts/permit-scripts.js"
  },
  "engines": {
    "node": "16.16.0"
  },
  "devDependencies": {
    "@types/node": "18.11.10",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.8",
    "@typescript-eslint/eslint-plugin": "5.45.0",
    "@typescript-eslint/parser": "5.45.0",
    "autoprefixer": "10.4.13",
    "chmod": "0.2.1",
    "eslint": "8.29.0",
    "eslint-config-next": "13.0.6",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "postcss": "8.4.19",
    "prettier": "2.8.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "shared-config": "*",
    "shared-ui": "*",
    "tailwindcss": "3.2.4",
    "turbo": "1.6.3",
    "typescript": "4.9.3"
  },
  "packageManager": "yarn@3.3.0"
}
