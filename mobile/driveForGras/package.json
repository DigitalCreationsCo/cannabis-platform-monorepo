{
	"name": "@cd/mobile-driveforgras",
	"version": "0.0.1",
	"scripts": {
		"prebuild": "npx expo prebuild",
		"build:deps": "yarn workspaces foreach -t --from '@cd/mobile-driveforgras' run build",
		"start": "NODE_ENV='development' expo start",
		"start:dev": "NODE_ENV='development' expo start --dev-client -c",
		"start:production": "NODE_ENV='production' npx expo start --no-dev --minify -c",
		"start-clear": "yarn build:deps && NODE_ENV='development' expo start -c",
		"android": "NODE_ENV='development' expo run:android",
		"ios": "NODE_ENV='development' expo run:ios",
		"web": "NODE_ENV='development' expo start --web",
		"test": "jest --watchAll",
		"test:unit": "echo test:unit",
		"lint": "eslint .",
		"lint-fix": "eslint . --fix",
		"clean": "rm -rf .expo ios/build android/app/build",
		"test:unit-ci": "echo \"Error: no test specified\" && exit 0"
	},
	"jest": {
		"preset": "jest-expo"
	},
	"dependencies": {
		"@cd/core-lib": "workspace:*",
		"@cd/data-access": "workspace:*",
		"@cd/eslint-config": "workspace:*",
		"@cd/native-ui": "workspace:*",
		"@expo/vector-icons": "13.0.0",
		"@react-native-async-storage/async-storage": "1.17.11",
		"@react-navigation/bottom-tabs": "6.0.9",
		"@react-navigation/drawer": "6.1.8",
		"@react-navigation/native": "6.0.2",
		"@react-navigation/stack": "6.0.11",
		"@reduxjs/toolkit": "1.9.2",
		"crypto-js": "4.1.1",
		"expo": "48.0.21",
		"expo-checkbox": "2.3.1",
		"expo-font": "11.1.1",
		"expo-linking": "4.0.1",
		"expo-location": "15.1.1",
		"expo-router": "1.5.2",
		"expo-splash-screen": "0.18.2",
		"expo-status-bar": "1.4.4",
		"expo-system-ui": "2.2.1",
		"expo-web-browser": "12.1.1",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-native": "0.71.14",
		"react-native-gesture-handler": "2.9.0",
		"react-native-safe-area-context": "4.5.0",
		"react-native-screens": "3.20.0",
		"react-native-svg": "13.4.0",
		"react-native-web": "0.18.10",
		"react-redux": "8.0.2",
		"redux-persist": "6.0.0",
		"supertokens-react-native": "4.0.8"
	},
	"devDependencies": {
		"@babel/core": "7.21.4",
		"@types/crypto-js": "4.1.1",
		"@types/react": "18.0.34",
		"@types/react-dom": "18.0.11",
		"@types/react-native": "0.72.2",
		"jest": "29.3.1",
		"jest-expo": "48.0.0",
		"react-native-dotenv": "3.4.9",
		"react-test-renderer": "18.2.0",
		"typescript": "4.9.3"
	},
	"private": true
}
