{
  "name": "@cd/mobile-driveforgras",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "installConfig": {
    "hoistingLimits": "workspaces"
  },
  "scripts": {
    "prebuild": "npx expo prebuild",
    "build:deps": "yarn workspaces foreach -t --from '@cd/mobile-driveforgras' run build",
    "start": "EXPO_USE_METRO_WORKSPACE_ROOT=1 NODE_ENV='development' expo start",
    "start:dev": "EXPO_USE_METRO_WORKSPACE_ROOT=1 NODE_ENV='development' expo start --dev-client -c",
    "start:production": "EXPO_USE_METRO_WORKSPACE_ROOT=1 NODE_ENV='production' npx expo start --no-dev --minify -c",
    "start:cold": "yarn clean && yarn build:deps && yarn prebuild && EXPO_USE_METRO_WORKSPACE_ROOT=1 NODE_ENV='development' expo start -c",
    "android": "EXPO_USE_METRO_WORKSPACE_ROOT=1 NODE_ENV='development' expo run:android",
    "ios": "EXPO_USE_METRO_WORKSPACE_ROOT=1 NODE_ENV='development' expo run:ios",
    "web": "EXPO_USE_METRO_WORKSPACE_ROOT=1 NODE_ENV='development' expo start --web",
    "test": "jest --watchAll",
    "test:unit": "echo test:unit",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "clean": "rm -rf .expo ios/build android/app/build",
    "test:unit-ci": "echo \"Error: no test specified\" && exit 0"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@cd/core-lib": "workspace:*",
    "@cd/data-access": "workspace:*",
    "@cd/eslint-config": "workspace:*",
    "@expo/vector-icons": "14.0.0",
    "@react-native-async-storage/async-storage": "1.21.0",
    "@react-navigation/drawer": "6.6.7",
    "@react-navigation/native": "6.0.2",
    "@realm/react": "0.6.2",
    "@reduxjs/toolkit": "2.2.1",
    "axios": "1.2.1",
    "crypto-js": "4.2.0",
    "expo": "50.0.7",
    "expo-checkbox": "2.7.0",
    "expo-font": "11.10.3",
    "expo-linking": "6.2.2",
    "expo-location": "16.5.3",
    "expo-router": "3.4.7",
    "expo-splash-screen": "0.26.4",
    "expo-status-bar": "1.11.1",
    "expo-system-ui": "2.9.3",
    "expo-web-browser": "12.8.2",
    "formik": "2.2.9",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.4",
    "react-native-gesture-handler": "2.14.0",
    "react-native-maps": "1.10.0",
    "react-native-reanimated": "3.6.2",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "3.29.0",
    "react-native-svg": "14.1.0",
    "react-native-web": "0.19.6",
    "react-redux": "8.0.2",
    "realm": "12.3.1",
    "redux-persist": "6.0.0",
    "supertokens-react-native": "4.0.8",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@babel/core": "7.21.4",
    "@types/crypto-js": "4.2.2",
    "@types/react": "18.2.45",
    "@types/react-dom": "18.0.11",
    "@types/react-native": "0.73.0",
    "babel-plugin-module-resolver": "5.0.0",
    "babel-preset-expo": "10.0.1",
    "jest": "29.2.1",
    "jest-expo": "50.0.2",
    "react-native-dotenv": "^3.4.10",
    "react-test-renderer": "18.2.0",
    "typescript": "4.9.3"
  },
  "private": true
}
