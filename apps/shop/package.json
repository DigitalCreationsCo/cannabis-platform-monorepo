{
  "name": "@cd/shop",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "NODE_ENV=production next build",
    "build:css": "npx tailwindcss -o ./src/index.css --minify",
    "postcss": "postcss ./src/styles/index.css -o ./src/styles/index.css",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "dev": "next dev",
    "uat": "NODE_ENV=uat next build && NODE_ENV=uat next start",
    "lint": "next lint",
    "clean": "rimraf ./.next ./out ./coverage ./tsconfig.tsbuildinfo ./node_modules/.cache",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "test": "jest --config ./jest.config.js",
    "test-watch": "jest --config ./jest.config.js --watchAll",
    "start": "pm2-runtime ecosystem.config.js"
  },
  "browserslist": {
    "production": [
      ">0.3%",
      "not ie 11",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@cd/core-lib": "workspace:*",
    "@cd/data-access": "workspace:*",
    "@cd/eslint-config": "workspace:*",
    "@cd/ui-lib": "workspace:*",
    "@paralleldrive/cuid2": "^2.2.0",
    "@reduxjs/toolkit": "1.9.2",
    "axios": "1.2.1",
    "boarding.js": "3.5.2",
    "cors": "2.8.5",
    "date-fns": "2.29.3",
    "form-data": "4.0.0",
    "json-immutable": "0.5.0",
    "next": "13.0.6",
    "next-connect": "0.13.0",
    "next-redux-wrapper": "8.1.0",
    "next-transpile-modules": "10.0.0",
    "pm2": "^5.2.2",
    "react": "18.2.0",
    "react-confetti": "6.1.0",
    "react-cookie": "4.1.1",
    "react-dom": "18.2.0",
    "react-dropzone": "14.2.3",
    "react-google-recaptcha": "2.1.0",
    "react-hot-toast": "2.4.0",
    "react-redux": "8.0.2",
    "react-responsive-carousel": "3.2.23",
    "sharp": "0.32.1",
    "tailwind-merge": "1.8.0",
    "use-carousel-hook": "^0.2.1"
