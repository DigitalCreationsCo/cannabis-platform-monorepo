{
  "name": "@cd/weed-dash",
  "version": "1.0.0",
  "scripts": {
    "serve": "NODE_PATH='../../' && bundle exec jekyll serve",
    "dev": "NODE_PATH='../../' && bundle exec jekyll serve --livereload --trace",
    "build": "NODE_PATH='../../' && bundle exec jekyll build",
    "watch": "NODE_PATH='../../' && bundle exec jekyll build --watch"
  },
  "keywords": [
    "ruby",
    "jekyll",
    "static",
    "markdown",
    "gitlab-pages"
  ],
  "license": "ISC",
  "description": "A directory site for weed delivery services built with Jekyll site bundler.",
  "devDependencies": {
    "autoprefixer": "10.4.19",
    "cssnano": "^7.0.2",
    "postcss": "8.4.38",
    "tailwindcss": "3.4.3"
  }
}
