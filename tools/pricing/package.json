{
  "name": "@gras/pricing-tool",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "rm -rf ./.next && yarn build:css && next build",
    "build:ci": "rm -rf ./.next && yarn build:css && next build",
    "build:css": "npm run copy-shared-assets && npx tailwindcss -i ./styles/globals.css -o ./styles/tailwind.css --minify",
    "copy-shared-assets": "sh ../../scripts/copysharedCSSandAssets.sh",
    "clean": "rm -rf ./.next ./out ./dist ./build ./node_modules ./coverage ./tsconfig.tsbuildinfo",
    "debug": "NODE_OPTIONS='--inspect' next dev -p 3008",
    "dev": "next-dev",
    "lint": "next lint",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "next-dev": "next dev -p 3008",
    "start": "PORT=3008 node server.js"
  },
  "packageManager": "yarn@3.3.1",
  "browserslist": {
    "production": [
      ">0.3%",
      "not ie 11",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@gras/core": "workspace:*",
    "@gras/data-access": "workspace:*",
    "@gras/eslint-config": "workspace:*",
    "@gras/ui": "workspace:*",
    "axios": "1.2.1",
    "boarding.js": "3.5.2",
    "cors": "2.8.5",
    "find-up": "5.0.0",
    "framer-motion": "10.15.0",
    "next": "14.2.6",
    "next-connect": "0.13.0",
    "nextjs-cors": "2.1.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "2.4.1",
    "swr": "2.2.5",
    "tailwind-merge": "1.8.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "@next/env": "13.2.4",
    "@tailwindcss/typography": "0.5.12",
    "@testing-library/dom": "^10.1.0",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/enzyme": "3.10.13",
    "@types/formidable": "2.0.5",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.8",
    "@types/react-dom": "18.3.1",
    "autoprefixer": "10.4.19",
    "babel-jest": "29.5.0",
    "daisyui": "4.10.2",
    "dotenv": "16.0.3",
    "dotenv-cli": "7.0.0",
    "dotenv-expand": "10.0.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.7",
    "enzyme-to-json": "3.6.2",
    "eslint": "^9.15.0",
    "formidable": "2.1.1",
    "formik": "2.4.5",
    "jest": "^29.7.0",
    "jest-mock-extended": "3.0.1",
    "jest-transform-stub": "2.0.0",
    "node-cache": "5.1.2",
    "node-fetch": "3.3.2",
    "node-mocks-http": "1.12.1",
    "npm-run-all": "4.1.5",
    "postcss": "^8.4.38",
    "postcss-cli": "10.1.0",
    "prettier": "^3.3.2",
    "rimraf": "4.4.1",
    "tailwindcss": "3.4.3",
    "ts-jest": "^29.1.5",
    "typescript": "5.5.2",
    "yup": "0.32.11"
  }
}
