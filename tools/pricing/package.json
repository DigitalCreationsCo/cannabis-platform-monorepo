{
	"name": "@cd/pricing-tool",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"build": "yarn build:css && rm -rf ./.next && NODE_ENV=production next build",
		"build:ci": "yarn build:css && rm -rf ./.next && NODE_ENV=production next build",
		"build:css": "tailwindcss -i ./src/styles/pricing-tool.css -o ./src/styles/tailwind.css",
		"clean": "rimraf ./src/styles/tailwind.css ./.next ./out ./dist ./build ./node_modules ./coverage ./tsconfig.tsbuildinfo ./node_modules/.cache",
		"debug": "NODE_OPTIONS='--inspect' next dev -p 3008",
		"dev": "npm-run-all --parallel --print-label watch-tailwind next-dev",
		"lint": "next lint",
		"lint:fix": "eslint . --ext .ts,.tsx --fix",
		"next-dev": "next dev -p 3008",
		"start": "PORT=3008 node server.js",
		"watch-tailwind": "tailwindcss -i ./src/styles/pricing-tool.css -o ./src/styles/tailwind.css --watch"
	},
	"packageManager": "yarn@3.3.1",
	"browserslist": {
		"production": [
			">0.3%",
			"not ie 11",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"dependencies": {
		"@cd/core-lib": "workspace:*",
		"@cd/data-access": "workspace:*",
		"@cd/eslint-config": "workspace:*",
		"@cd/ui-lib": "workspace:*",
		"@reduxjs/toolkit": "1.9.2",
		"axios": "1.2.1",
		"boarding.js": "3.5.2",
		"cors": "2.8.5",
		"find-up": "5.0.0",
		"framer-motion": "10.15.0",
		"next": "13.4.16",
		"next-connect": "0.13.0",
		"next-redux-wrapper": "8.1.0",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-hot-toast": "2.4.0",
		"react-redux": "8.0.2",
		"sharp": "0.32.1",
		"swr": "2.2.4",
		"tailwind-merge": "1.8.0"
	},
	"devDependencies": {
		"@babel/preset-env": "7.21.4",
		"@babel/preset-react": "7.22.5",
		"@next/env": "13.2.4",
		"@tailwindcss/typography": "0.5.8",
		"@testing-library/dom": "9.3.1",
		"@testing-library/jest-dom": "5.16.5",
		"@testing-library/react": "14.0.0",
		"@testing-library/user-event": "14.4.3",
		"@types/enzyme": "3.10.13",
		"@types/formidable": "2.0.5",
		"@types/jest": "29.2.4",
		"@types/node": "18.11.10",
		"@types/react-dom": "18.0.11",
		"@types/react-google-recaptcha": "2.1.5",
		"@types/redux-mock-store": "^1.0.3",
		"@typescript-eslint/eslint-plugin": "5.45.0",
		"@typescript-eslint/parser": "5.45.0",
		"autoprefixer": "10.4.13",
		"babel-jest": "29.5.0",
		"daisyui": "2.51.6",
		"dotenv": "16.0.3",
		"dotenv-cli": "7.0.0",
		"dotenv-expand": "10.0.0",
		"enzyme": "3.11.0",
		"enzyme-adapter-react-16": "1.15.7",
		"enzyme-to-json": "3.6.2",
		"eslint": "8.29.0",
		"eslint-config-next": "13.0.6",
		"eslint-plugin-import": "2.26.0",
		"eslint-plugin-jsx-a11y": "6.6.1",
		"eslint-plugin-prettier": "4.2.1",
		"eslint-plugin-react": "7.31.11",
		"eslint-plugin-react-hooks": "4.6.0",
		"eslint-plugin-simple-import-sort": "8.0.0",
		"formidable": "2.1.1",
		"formik": "2.2.9",
		"jest": "29.3.1",
		"jest-environment-jsdom": "29.3.1",
		"jest-junit": "16.0.0",
		"jest-mock-extended": "3.0.1",
		"jest-transform-stub": "2.0.0",
		"node-cache": "5.1.2",
		"node-fetch": "3.3.2",
		"node-mocks-http": "1.12.1",
		"npm-run-all": "4.1.5",
