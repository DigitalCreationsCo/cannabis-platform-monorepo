{
    "$schema": "https://turbo.build/schema.json",
    "pipeline": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "inputs": [],
            "outputs": [
                "dist/**",
                ".next/**"
            ]
        },
        "build:prod": {
            "outputs": [
                "dist/**",
                ".next/**"
            ]
        },
        "deploy": {
            "dependsOn": [
                "build",
                "test",
                "lint"
            ],
            "outputs": []
        },
        "dev": {
            "cache": false,
            "dependsOn": []
        },
        "lint": {
            "outputs": []
        },
        "lint:fix": {
            "outputs": []
        },
        "test": {
            "inputs": [
                "**/*.test.*"
            ],
            "outputs": []
        },
        "watch": {
            "cache": true,
            "dependsOn": [],
            "inputs": [],
            "outputs": []
        }
    }
}