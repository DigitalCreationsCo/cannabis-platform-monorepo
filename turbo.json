{
    "$schema": "https://turbo.build/schema.json",
    "globalDependencies": ["./.env.development"],
    "globalEnv": ["NODE_ENV", "BABEL_ENV"],
    "pipeline": {
        "@cd/data-access#db:generate": {},
        "build": {
            "dependsOn": [
                "^build"
            ],
            "inputs": [],
            "outputs": [
                "dist/**",
                ".next/**"
            ]
        },
        "build:prod": {
            "outputs": [
                "dist/**",
                ".next/**"
            ]
        },
        "deploy": {
            "dependsOn": [
                "build",
                "test",
                "lint"
            ],
            "outputs": []
        },
        "dev": {
            "cache": false,
            "dependsOn": []
        },
        "lint": {
            "outputs": []
        },
        "lint:fix": {
            "outputs": []
        },
        "test": {
            "inputs": [
                "**/*.test.*"
            ],
            "outputs": []
        },
        "watch": {
            "cache": false,
            "dependsOn": ["^watch"],
            "inputs": [],
            "outputs": [
                "dist/**",
                ".next/**"
            ]
        }
    }
}