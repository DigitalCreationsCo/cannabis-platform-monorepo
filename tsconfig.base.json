{
	"compilerOptions": {
		"strict": true,
		"useUnknownInCatchVariables": false,
		"allowJs": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"noEmit": true,
		"esModuleInterop": true,
		"moduleResolution": "Node16",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"incremental": true,
		"newLine": "lf"
	},
	"exclude": ["**/node_modules", "**/.*/"]
}
